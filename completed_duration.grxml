        <item><ruleref uri="#DurationRel"/><tag><![CDATA[if(typeof(DurationRel.DURATION)!='undefined' && typeof(DurationRel.DURATION.DURATION_REL)!='undefined')DURATION={'DURATION_REL':DurationRel.DURATION.DURATION_REL}]]></tag></item>
