<tag> DURATION5 = {} </tag> 
            <tag><![CDATA[SECOND = "?"; MSECOND = "?"; MINUTE = "?"; HOUR = "?"; DAY = "?"; WEEK = "?"; MONTH = "?"; YEAR = "?"; M = "?"; ]]></tag>
            <item repeat="0-1">
                <ruleref uri="#duration_keyword"/>
            </item>
