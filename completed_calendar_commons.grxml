        <item repeat="0-1"> dalam </item>
        <ruleref uri="#Year"/> <tag><![CDATA[Y=Year.XY;DATE={'DATE_ABS':{'YEAR':Y}};]]></tag>
        <one-of>
                 <item> esok  <tag><![CDATA[CC=1; DT= {'INCREMENT':CC} ; DT.STEP="day"; ]]></tag></item>
                <item> hari ini  <tag><![CDATA[CC=0; DT= {'INCREMENT':CC} ; DT.STEP="day"; ]]></tag></item>
                <item> semalam  <tag><![CDATA[CC=-1; DT= {'INCREMENT':CC} ; DT.STEP="day"; ]]></tag></item>
                <item>
                  <ruleref uri="#Cardinal1_120"/> <tag><![CDATA[C=parseInt(Cardinal1_120.XY);DT= {'INCREMENT':CC} ; DT.STEP="day"; ]]></tag>
                 hari lalu
               </item>
         </one-of>
         <tag><![CDATA[if(typeof(DATE)!='undefined'){ DATE.DATE_REL=DT; } else {DATE={'DATE_REL':DT}; }]]></tag>
      </item> 
    </one-of>
</rule>


</grammar>
