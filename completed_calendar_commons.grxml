	<ruleref uri="#DateCore"/> <tag><![CDATA[M=DateCore.M; D=DateCore.D]]></tag>       
	<item repeat="0-1"> 
             <item repeat="0-1"> 
                <one-of>  
                   <item> bagi </item>
                   <item> dalam </item>
                </one-of>
             </item>
             <ruleref uri="#Year"/> <tag><![CDATA[Y=Year.XY]]></tag>
       </item>
	<tag><![CDATA[XY= {'DAY':D} ; if(M!="?"){XY.MONTH=M;} if(Y!="?"){XY.YEAR=Y;} XY= {'DATE_ABS':XY}; delete M; delete Y; delete D; if(typeof(DR)!='undefined'){ XY.DATE_REL=DR; }]]></tag>
      </item>
      <!-- inverse: year + date -->
      <item>
          <item repeat="0-1"> dalam </item>
          <ruleref uri="#Year"/> <tag><![CDATA[Y=Year.XY]]></tag>
          <item repeat="0-1"> pada </item>
          <ruleref uri="#DateCore"/> <tag><![CDATA[M=DateCore.M; D=DateCore.D]]></tag>
          <tag><![CDATA[XY= {'DAY':D} ; if(M!="?"){XY.MONTH=M;} if(Y!="?"){XY.YEAR=Y;} XY= {'DATE_ABS':XY}; delete M; delete Y; delete D; if(typeof(DR)!='undefined'){ XY.DATE_REL=DR; }]]></tag>
      </item>
      <item> <!-- sunday the third -->
         <ruleref uri="#DayOfWeek"/><tag><![CDATA[DR={'DAY_OF_WEEK':DayOfWeek.XY};]]></tag>
         yang 
         <item><ruleref uri="#OrdinalDay"/> <tag><![CDATA[D=OrdinalDay.XY]]></tag></item>
         <tag><![CDATA[XY= {'DATE_ABS':{'DAY':D}} ; if(typeof(DR)!='undefined'){ XY.DATE_REL=DR; } delete M; delete Y; delete D;]]></tag>
      </item>
      <item>
 	   <item repeat="0-1"> yang </item>
           <item><ruleref uri="#OrdinalDay"/> <tag><![CDATA[D=OrdinalDay.XY]]></tag></item>
                
             hari bagi
           <item> <ruleref uri="#NamedRange"/> <tag><![CDATA[M=NamedRange.XY]]></tag></item>
           <item repeat="0-1"> 
             <item repeat="0-1"> 
                <one-of> 
                   <item> bagi </item> 
                   <item> dalam </item>
                </one-of>
             </item>
             <ruleref uri="#Year"/> <tag><![CDATA[Y=Year.XY]]></tag>
       </item>

           <tag><![CDATA[XY= {'DAY':D} ; if(M!="?"){XY.NAMED_RANGE=M;} if(Y!="?"){XY.YEAR=Y;} XY= {'DATE_ABS':XY}; delete M; delete Y; delete D;]]></tag>
      </item>       
      <item>
         pada 
          <ruleref uri="#DateMinimal"/> <tag><![CDATA[XY= {'DATE_ABS':{'DAY':DateMinimal.XY}} ;]]></tag>
      </item>
      <item> <!-- second day of 2020; -->
         <ruleref uri="#DateMinimal"/> hari <tag><![CDATA[XY= {'DATE_ABS':{'DAY':DateMinimal.XY}} ;]]></tag> 
         <one-of>
            <item> bagi </item>
            <item> dalam </item>
         </one-of>
         <ruleref uri="#Year"/> <tag><![CDATA[XY.DATE_ABS.YEAR=Year.XY; if(DateMinimal.XY<=31){XY.DATE_ABS.MONTH=1;}]]></tag>
      </item>
      <item> <!-- last day in 2020 (== Dec 31) --> 
         hari terakhir <tag><![CDATA[XY= {'DATE_ABS':{'DAY':31,'MONTH':12}} ;]]></tag> 
         <one-of>
            <item> bagi </item>
            <item> dalam </item>
         </one-of>
         <ruleref uri="#Year"/> <tag><![CDATA[XY.DATE_ABS.YEAR=Year.XY;]]></tag>
      </item>
      <item>
	<item><ruleref uri="#NamedDay"/> <tag><![CDATA[D=NamedDay.XY]]></tag></item>
        <item> 
