                </one-of>
             </item>
             <ruleref uri="#Year"/> <tag><![CDATA[Y=Year.XY]]></tag>
       </item>
	<tag><![CDATA[XY= {'DAY':D} ; if(Y!="?"){XY.YEAR=Y;} XY= {'DATE_ABS':XY}; delete M; delete Y; delete D;]]></tag>
      </item>
      <item>
